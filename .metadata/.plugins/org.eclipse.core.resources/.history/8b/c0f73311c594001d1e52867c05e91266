package com.vcanus.test04;

public class Run {

	public static void main(String[] args) {
		long result = factorial(17);
		System.out.println(result);
	}

	// 재귀함수를 반복문으로 변경
	public static long factorial(long num) {
		long result = 1;
		
		while (num != 0) {
			result *= num--;
		}
		return result;
	}

}
