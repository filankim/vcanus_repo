package com.vcanus.test04;

public class Run {

	public static void main(String[] args) {
		long result = factorial(30);
		System.out.println(result);
	}

	//스택오버플로우 방지
	//방법1. 재귀함수를 반복문으로 변경
	public static long factorial(long num) {
		long result = 1L;
		
		while (num != 0) {
			result *= num--;
		}
		
		return result;
	}

}
