package com.vcanus.test03;

public class Run {

	public static void main(String[] args) {
		int result = factorial(10);
		System.out.println(result);
	}
	
	public static int factorial(int num) {
		if(num<=1) {
			return 1;
		}
		return num * factorial(num-1);
	}
	public static int factorial(int num) {
		if(num<=1) {
			return 1;
		}
		return num * factorial(num-1);
	}

}
